; (set-logic QF_NRA)
(set-option :precision 0.001)

(declare-fun A_0_0 () Real)
(declare-fun A_0_1 () Real)
(declare-fun A_1_0 () Real)
(declare-fun A_1_1 () Real)
(declare-fun A_2_0 () Real)
(declare-fun d_0 () Real)
(declare-fun err () Real)
(declare-fun fx_0 () Real)
(declare-fun norm_o () Real)
(declare-fun x_0 () Real)


(assert (>= x_0 -3.15000))
(assert (<= x_0 3.15000))
(assert (= A_0_0 (* 1.00000 x_0)))
(assert (= A_0_1 (* -1.00000 x_0)))
(assert (=> (>= A_0_0 0) (= A_1_0 A_0_0)))
(assert (=> (< A_0_0 0) (= A_1_0 0)))
(assert (=> (>= A_0_1 0) (= A_1_1 A_0_1)))
(assert (=> (< A_0_1 0) (= A_1_1 0)))
(assert (= A_2_0 (+ (* 1.00000 A_1_0) (* -1.00000 A_1_1))))
(assert (= fx_0 x_0))
(assert (=> (>= (+ A_2_0 (* -1 fx_0)) 0) (= d_0 (+ A_2_0 (* -1 fx_0)))))
(assert (=> (< (+ A_2_0 (* -1 fx_0)) 0) (= d_0 (+ fx_0 (* -1 A_2_0)))))
(assert (= norm_o d_0))
(assert (= err norm_o))
(assert (not (= err 0)))

(check-sat)
; (get-model)
(exit)
